view , page {
  padding: 0px;
  margin: 0px;
  box-sizing: border-box;
}

page{
  width: 750rpx;
  font-size: 30rpx;
  color: #5f5f5f;
  position: relative;
  /* height: 100%; */
  background-color: #f5f6fa;
}

.mask{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 5000000;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .4);
}

.receivable{
  position: absolute;
  left: calc(50vw - 286rpx);
  bottom: calc(50vh - 218.5rpx);
  width: 572rpx;
  height: 467rpx;
  background: #fff;
  overflow: hidden;
  text-align: center;
  padding: 62rpx 0 0;
}
.faceToFace{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 80rpx;
  margin-bottom: 60rpx;
  color:#6b6b6b;
}


.succe{
  width: 131rpx;
  height: 131rpx;
  vertical-align: top;
}

.back{
  width: 100%;
  border-top:1rpx solid #eeeeee;
  text-align: center;
  color:#ca161f;
  font-size: 25rpx;
  padding: 28rpx 0;
}

